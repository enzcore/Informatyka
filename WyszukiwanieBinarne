def wyszukiwanie_binarne(lista, szukana):
    lewy = 0
    prawy = len(lista) - 1

    while lewy <= prawy:
        srodek = (lewy + prawy) // 2

        if lista[srodek] == szukana:
            return srodek
        elif lista[srodek] < szukana:
            lewy = srodek + 1
        else:
            prawy = srodek - 1

    return -1
